<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorWhite"
    tools:context=".ui.fragments.notLoggedIn.historyAndReading.notLoggedInReservations.NotLoggedInReservations">


    <ImageView
        android:id="@+id/notLoggedInReservations_image_LoginIn"
        android:layout_width="190dp"
        android:layout_height="170dp"
        android:layout_marginTop="140dp"
        android:clickable="true"
        android:contentDescription="@string/clicktologinin"
        android:focusable="true"
        android:src="@drawable/not_login_reserv"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/button"
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:layout_marginTop="50dp"
        android:text="@string/login"
        android:textAlignment="center"
        android:textColor="@color/colorPrimaryGreen"
        android:textSize="26sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="334dp"
        android:layout_height="69dp"
        android:layout_marginTop="50dp"
        android:text="@string/create_your_own_reservation_list"
        android:textAlignment="center"
        android:textColor="@color/colorPrimaryGreen"
        android:textSize="26sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/notLoggedInReservations_image_LoginIn" />

</androidx.constraintlayout.widget.ConstraintLayout>